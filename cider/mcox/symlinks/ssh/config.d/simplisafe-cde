  User mcox
  IdentityFile ~/.ssh/simplisafe/aws_iam_mcox

# Nitro
Host nitro.prd.simplisafe.com 10.20.0.136
  User mcox
  IdentityFile ~/.ssh/simplisafe/aws_iam_mcox
